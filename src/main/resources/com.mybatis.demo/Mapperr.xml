<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.mybatis.demo.Mapperr">
    <select id="list" resultType="com.mybatis.demo.Test">
        select * from TEST
    </select>
    <insert id="insert" parameterType="com.mybatis.demo.Test">
        INSERT INTO TEST (ID_TEST)
        VALUES (#{id_test})
    </insert>
</mapper>